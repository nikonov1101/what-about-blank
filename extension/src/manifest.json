{
  "name": "Dashboard New Tab",
  "version": "1.0.0",
  "short_name": "dashboard-extension",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self' 'unsafe-eval'"
}
